    <managed-bean>
        <managed-bean-name>showAbonents</managed-bean-name>
        <managed-bean-class>com.ruslik.catalog.model.impl.AbonentServiceImpl</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
